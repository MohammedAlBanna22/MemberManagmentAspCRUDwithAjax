@model List<AddMemeberModel>
<html>
<head>
</head>
<body>

    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th scope="col">MemberID</th>
                <th scope="col">Status</th>
                <th scope="col">FirstName</th>
                <th scope="col">LastName</th>
                <th scope="col">Country</th>
                <th scope="col">Gender</th>
                <th scope="col">Notes</th>


            </tr>
        </thead>











        <tbody>
            @{int id = 0; }
            @foreach (var x in Model)
            {

                <tr>
                    <th scope="row">
                        @(++id)
                    </th>
                    <td>@x.status</td>
                    <td>@x.FirstName</td>
                    <td>@x.LastName</td>
                    <td>@x.Country</td>
                    <td>@x.gender</td>

                    <td>@x.Note</td>
                    @*<td><img src="@x.imageurl" style="height:40px;width:40px" /></td>*@






                </tr>
            }

        </tbody>
    </table>
    <button type="submit" class="btn-primary" style="margin-left:500px">Add Memmber</button>



</body>
</html>


